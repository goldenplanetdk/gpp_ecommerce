<productfeed>
    {% for product in products %}
        <product>
            <productname>{{ product.title }}</productname>
            <brandname>{{ product.brand }}</brandname>
            <MPN>{{ product.model }}</MPN>
            <EAN>{{ product.ean }}</EAN>
            <stock>{{ product.quantity }}</stock>
            <price>{{ product.price_with_tax }}</price>
            <category_name>{{ product.category }}</category_name>
            <your_product_ID>{{ product.id }}</your_product_ID>
            <image_URL>{{ product.image ? absolute_url(product.image) : '' }}</image_URL>
            <product_URL>{{ absolute_url(product.url) }}</product_URL>
        </product>
    {% endfor %}
</productfeed>
