<productfeed>
    {% for product in products %}
        <product>
            <productname>{{ product.title }}</productname>
            <brandname>{{ product.brand }}</brandname>
            <MPN>{{ product.mpn }}</MPN>
            <EAN>{{ product.gtin }}</EAN>
            <stock>{{ product.quantity }}</stock>
            <price>{{ product.price.value }}</price>
            <category_name>{{ product.category }}</category_name>
            <your_product_ID>{{ product.id }}</your_product_ID>
            <image_URL>{{ product.image_link }}</image_URL>
            <product_URL>{{ product.link  }}</product_URL>
        </product>
    {% endfor %}
</productfeed>
